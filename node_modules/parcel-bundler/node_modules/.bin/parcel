#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/sandbox/node_modules/.pnpm/parcel-bundler@1.6.1/node_modules/parcel-bundler/bin/node_modules:/workspaces/sandbox/node_modules/.pnpm/parcel-bundler@1.6.1/node_modules/parcel-bundler/node_modules:/workspaces/sandbox/node_modules/.pnpm/parcel-bundler@1.6.1/node_modules:/workspaces/sandbox/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/sandbox/node_modules/.pnpm/parcel-bundler@1.6.1/node_modules/parcel-bundler/bin/node_modules:/workspaces/sandbox/node_modules/.pnpm/parcel-bundler@1.6.1/node_modules/parcel-bundler/node_modules:/workspaces/sandbox/node_modules/.pnpm/parcel-bundler@1.6.1/node_modules:/workspaces/sandbox/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
